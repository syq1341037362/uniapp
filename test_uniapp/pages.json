{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh":true
			}
		},{
		    "path" : "pages/news/news",
		    "style" : {
				"navigationBarTitleText": "新闻"
			}
		},{
				"path" : "pages/test/test",
				"style" : {
				"navigationBarTitleText": "测试"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"backgroundTextStyle":"dark",
		"onReachBottomDistance":50
	},
	"tabBar": {
	    "color": "#8F8F94",
	    "selectedColor": "#1296db",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [{
	        "pagePath": "pages/index/index",
			"iconPath":"static/img/index.png",
			"selectedIconPath":"static/img/select_index.png",
	        "text": "首页"
	    }, {
	        "pagePath": "pages/news/news",
			"iconPath":"static/img/new.png",
			"selectedIconPath":"static/img/select_new.png",
	        "text": "新闻"
	    }, {
	        "pagePath": "pages/test/test",
			"iconPath":"static/img/test.png",
			"selectedIconPath":"static/img/select_test.png",
	        "text": "测试"
	    }]
	}
}
